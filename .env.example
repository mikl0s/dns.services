# Required settings
DNS_SERVICES_USERNAME="your_username"    # DNS.services account username
DNS_SERVICES_PASSWORD="your_password"    # DNS.services account password
DNS_SERVICES_BASE_URL="https://dns.services"  # API base URL

# Optional settings
DNS_SERVICES_TOKEN_PATH="~/.dns-services/token"  # JWT token storage location
DNS_SERVICES_VERIFY_SSL="true"  # Enable/disable SSL verification
DNS_SERVICES_TIMEOUT="30"  # API timeout in seconds
DNS_SERVICES_DEBUG="false"  # Enable debug logging
